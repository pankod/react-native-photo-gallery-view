{
	"compilerOptions": {
		"allowJs": false,
		"baseUrl": ".",
		"rootDirs": [
			"./src"
		],
		"allowSyntheticDefaultImports": true,
		"noFallthroughCasesInSwitch": true,
		"experimentalDecorators": true,
		"esModuleInterop": true,
		"jsx": "react-native",
		"paths": {},
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es6"
		],
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"target": "esnext"
	},
	"exclude": [
		"node_modules"
	]
}
